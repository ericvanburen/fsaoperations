Update table 'Calls' Add field name 'NewAssignmentID (int)'

UPDATE Calls
Set Calls.NewAssignmentID = [PCACalls_Assignments].[NewAssignmentID]
FROM         PCACalls_Assignments, Calls
WHERE 
		Calls.UserID = [PCACalls_Assignments].UserID 
		AND Calls.PCAID = [PCACalls_Assignments].PCAID 
		AND Calls.RecordingDeliveryDate = [PCACalls_Assignments].RecordingDeliveryDate



